Plug 'kchmck/vim-coffee-script'
" rails.vim: Ruby on Rails power tools
" http://www.vim.org/scripts/script.php?script_id=1567
Plug 'tpope/vim-rails'
" Markdown Vim Mode
" http://plasticboy.com/markdown-vim-mode/
Plug 'plasticboy/vim-markdown'
